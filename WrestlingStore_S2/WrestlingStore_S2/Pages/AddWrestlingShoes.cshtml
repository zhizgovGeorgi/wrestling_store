@page
@model WrestlingStore_S2.Pages.AddWrestlingShoesModel
@{
    ViewData["Title"] = "AddWrestlingShoes";
}

<!DOCTYPE html>
<html lang="en">




<body>

    <header>

        <form method="post">


            <div class="addProd">



                <label> Product Name:</label> <span asp-validation-for="ws.ProdName"></span>
                <input asp-for="ws.ProdName" id="name">
                <label> Product Category:</label> <span asp-validation-for="ws.ProdCategory"></span>
                <input asp-for="ws.ProdCategory" id="name">
                <label> Product Price:</label> <span asp-validation-for="ws.ProdPrice"></span>
                <input asp-for="ws.ProdPrice" id="price">
                <label> Product Image:</label> <span asp-validation-for="ws.ProdImg"></span>
                <input asp-for="ws.ProdImg" id="text">
                <label> Kind:</label> <span asp-validation-for="ws.ProdKind"></span>
                <input asp-for="ws.ProdKind" id="text">
                <input type="submit" value="Create Product" />


            </div>

        </form>



    </header>



</body>

</html>

@section scripts{
    <partial name="_ValidationScriptsPartial" />
}